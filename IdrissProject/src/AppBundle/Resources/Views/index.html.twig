<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Responsive Admin Dashboard Template">
    <meta name="keywords" content="admin,dashboard">
    <meta name="author" content="stacks">
    <!-- The above 6 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <!-- Title -->
    <title>WorldCupApp</title>
    {% block stylesheets %}
        {% stylesheets 'app/plugins/bootstrap/css/bootstrap.min.css'
            'app/plugins/font-awesome/css/font-awesome.min.css'
            'app/plugins/icomoon/style.css'
            'app/plugins/uniform/css/default.css'
            'app/plugins/switchery/switchery.min.css'
            'app/plugins/datatables/css/jquery.datatables.min.css'
            'app/plugins/datatables/css/jquery.datatables_themeroller.css'
            'app/plugins/bootstrap-datepicker/css/datepicker3.css'
            filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        <!-- Styles -->
        <link href="https://fonts.googleapis.com/css?family=Ubuntu" rel="stylesheet">


        <!-- Theme Styles -->
        {% stylesheets 'app/css/space.css'
            'app/css/custom.css'
            filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
    {% endblock %}
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>

    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
<body>
<a href="{{ path('fos_user_security_login') }}">login</a>
</body>

{% block javascripts %}
{% javascripts
    'app/plugins/jquery/jquery-3.1.0.min.js'
    'app/plugins/bootstrap/js/bootstrap.min.js'
    'app/plugins/jquery-slimscroll/jquery.slimscroll.min.js'
    'app/plugins/uniform/js/jquery.uniform.standalone.js'
    'app/plugins/switchery/switchery.min.js'
    'app/plugins/datatables/js/jquery.datatables.min.js'
    'app/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js'
    'app/js/pages/table-data.js'
    'app/js/space.min.js'
%}
    <script src="{{ asset_url }}"></script>
{% endjavascripts %}

</html>
{% endblock %}
